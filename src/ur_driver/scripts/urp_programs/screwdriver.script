  def rq_is_comm_lost(slave_id):
    rq_comm_read_registers_200x(slave_id, RQ_READ_SCREWDRIVER_REGISTERS_SIZE)
  
    return rq_comm_lost[rq_comm_slave_id_to_index(slave_id)]
  end
  
